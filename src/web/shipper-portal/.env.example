NODE_ENV=development

# API configuration
REACT_APP_API_URL=http://localhost:3000/api
REACT_APP_API_VERSION=v1
REACT_APP_API_TIMEOUT=30000
REACT_APP_API_BASE_URL=https://api.freightoptimization.com

# Authentication
REACT_APP_AUTH_DOMAIN=auth.freightoptimization.com
REACT_APP_AUTH_CLIENT_ID=shipper_portal
REACT_APP_AUTH_AUDIENCE=https://api.freightoptimization.com
REACT_APP_AUTH_SCOPE=openid profile email
REACT_APP_AUTH_TOKEN_EXPIRY=86400

# Feature flags
REACT_APP_FEATURE_CARRIER_RECOMMENDATIONS_ENABLED=true
REACT_APP_FEATURE_MARKET_INTELLIGENCE_ENABLED=true
REACT_APP_FEATURE_OPTIMIZATION_SAVINGS_ENABLED=true
REACT_APP_FEATURE_REAL_TIME_TRACKING_ENABLED=true

# Maps and location services
REACT_APP_MAPBOX_ACCESS_TOKEN=your_mapbox_access_token_here
REACT_APP_GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# Notifications
REACT_APP_FIREBASE_API_KEY=your_firebase_api_key_here
REACT_APP_FIREBASE_AUTH_DOMAIN=your_firebase_auth_domain_here
REACT_APP_FIREBASE_PROJECT_ID=your_firebase_project_id_here
REACT_APP_FIREBASE_STORAGE_BUCKET=your_firebase_storage_bucket_here
REACT_APP_FIREBASE_MESSAGING_SENDER_ID=your_firebase_messaging_sender_id_here
REACT_APP_FIREBASE_APP_ID=your_firebase_app_id_here

# Analytics
REACT_APP_ANALYTICS_ENABLED=true
REACT_APP_ANALYTICS_TRACKING_ID=your_analytics_tracking_id_here

# Performance monitoring
REACT_APP_SENTRY_DSN=your_sentry_dsn_here
REACT_APP_SENTRY_ENVIRONMENT=development

# UI configuration
REACT_APP_DEFAULT_LANGUAGE=en
REACT_APP_DEFAULT_THEME=light

# Tracking configuration
REACT_APP_TRACKING_WEBSOCKET_URL=wss://api.freightoptimization.com/tracking
REACT_APP_TRACKING_REFRESH_INTERVAL=30000

# TMS integration
REACT_APP_TMS_INTEGRATION_ENABLED=true
REACT_APP_TMS_PROVIDER_URLS='{"mcleod":"https://api.mcleod.com","tmw":"https://api.tmw.com","mercurygate":"https://api.mercurygate.com"}'

# Web socket configuration
REACT_APP_WEBSOCKET_URL=ws://localhost:3001
REACT_APP_WEBSOCKET_RECONNECT_INTERVAL=5000
REACT_APP_WEBSOCKET_MAX_RECONNECT_ATTEMPTS=10

# Build configuration
GENERATE_SOURCEMAP=true
SKIP_PREFLIGHT_CHECK=true
DISABLE_ESLINT_PLUGIN=false
TSC_COMPILE_ON_ERROR=true
REACT_APP_BUNDLE_VISUALIZE=false

# Shipper portal specific settings
REACT_APP_SHIPPER_DASHBOARD_REFRESH_INTERVAL=60000
REACT_APP_SHIPPER_SHIPMENT_MAP_DEFAULT_ZOOM=5
REACT_APP_SHIPPER_SHIPMENT_MAP_DEFAULT_CENTER='{"lat": 39.8283, "lng": -98.5795}'
REACT_APP_SHIPPER_MAX_LOADS_PER_PAGE=15
REACT_APP_SHIPPER_MAX_CARRIERS_PER_PAGE=20
REACT_APP_POSITION_CACHE_EXPIRY=30000
REACT_APP_TRAJECTORY_CACHE_EXPIRY=60000
REACT_APP_LOAD_CACHE_EXPIRY=300000
REACT_APP_LOAD_LIST_CACHE_EXPIRY=120000